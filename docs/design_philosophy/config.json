{
  "theme": "default",
  "themeVariables": {
    "fontFamily": "Inter, Helvetica, sans-serif",
    "fontSize": "16px",
    "background": "#ffffff",

    "primaryColor": "#0047a1",
    "primaryTextColor": "#ffffff",
    "primaryBorderColor": "#00397f",

    "secondaryColor": "#a04c00",
    "secondaryTextColor": "#ffffff",
    "secondaryBorderColor": "#823f00",

    "tertiaryColor": "#f5f5f5",
    "tertiaryTextColor": "#222222",
    "tertiaryBorderColor": "#cccccc",

    "lineColor": "#333333",
    "edgeLabelBackground": "#ffffff",
    "nodeTextColor": "#222222"
  },
  "flowchart": {
    "curve": "basis",
    "htmlLabels": true,
    "useMaxWidth": true
  },
  "sequence": {
    "mirrorActors": false,
    "actorFontSize": 16,
    "messageFontSize": 14
  }
}
